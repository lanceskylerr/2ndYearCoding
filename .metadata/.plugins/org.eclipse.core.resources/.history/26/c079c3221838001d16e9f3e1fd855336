import java.util.*;
public class Recap{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        
        String email[] = {"totoy@yahoo.com", "bimby@gmail.com", "josh@bing.com"};
        String username[] = {"TotoyBato", "El Bimby", "JoshyPa2T"};
        String password[] = {"matigasAko", "elChupakabra", "gayNaGay"};
        
        
        System.out.println("Index   : ");
        int id = input.nextInt();
        System.out.println("Email   : " + email[id]);
        System.out.println("Username: " + username[id]);
        System.out.println("Password: " + password[id]);
        
    }
}